UXTerm*background: rgb:0/0/0
UXTerm*foreground: rgb:c/5/0
!UXTerm*foreground: rgb:a0/a0/a0
UXTerm*loginShell: true
UXTerm*font: -schumacher-clean-medium-r-normal-*-12-120-75-75-c-60-iso10646-1

XEyes*geometry: 75x50-0+0
XBiff*geometry: 48x48-75+0
XClock*geometry: 82x82-123+0
XClock*update: 1
XLoad*geometry: 210x137-205+0
xosview*geometry: +0+0
XLoad*label:
XNetload*font: -*-*-*-*-*-*-6-*-*-*-*-*-*-*

URxvt.background: rgb:0/0/0
!URxvt.cursorBlink: True
URxvt.font: -schumacher-clean-medium-r-normal-*-12-120-75-75-c-60-iso10646-1
URxvt.boldFont: -schumacher-clean-bold-*-*-*-*-*-*-*-*-*-*-*
URxvt.foreground: rgb:f/6/0
URxvt.loginShell: True
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.underlineURLs: True
! https://github.com/muennich/urxvt-perls
URxvt.perl-ext-common: default,matcher,url-select
URxvt.perl-lib: /home/user/lib/urxvt/perl
URxvt.urlLauncher: /home/user/bin/browser.sh
URxvt.scrollBar: False

!xautolock*locker: slock
!xautolock*time: 3

XIdle*timeout: 180
XIdle*program: /usr/X11R6/bin/xlock

!*.font: -misc-fixed-medium-r-semicondensed-*-13-120-75-75-c-60-iso10646-1
!*.font: -schumacher-clean-medium-r-normal-*-12-120-75-75-c-60-iso10646-1
